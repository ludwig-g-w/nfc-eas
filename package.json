{
  "name": "uni-app",
  "version": "1.0.0",
  "main": "./src/index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "EXPO_USE_FAST_RESOLVER=1 npx expo --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prisma:studio": "npx dotenv-cli -e .env prisma studio",
    "prisma:seed": "npx dotenv-cli -e .env prisma db seed",
    "prisma:migrate": "npx dotenv-cli -e .env prisma migrate",
    "prisma:generate": "npx prisma generate --schema src/prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema src/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema src/prisma/schema.prisma",
    "prisma:introspect": "npx prisma introspect --schema src/prisma/schema.prisma",
    "e2e:android": "./scripts/run-android-e2e.sh",
    "e2e:web": "./scripts/run-web-e2e.sh",
    "build": "prisma generate && prisma migrate deploy && npx tailwindcss -i ./src/global.css -o ./node_modules/.cache/nativewind/global.css.web.css && EXPO_USE_FAST_RESOLVER=1 expo export -p web",
    "build:docker": "prisma generate && prisma migrate deploy && prisma db seed && npx tailwindcss -i /usr/src/app/src/global.css -o /usr/src/app/node_modules/.cache/nativewind/global.css.web.css && EXPO_USE_FAST_RESOLVER=1 expo export -p web"
  },
  "dependencies": {
    "@coinbase/wallet-mobile-sdk": "^1.0.13",
    "@gluestack-ui/divider": "latest",
    "@gluestack-ui/nativewind-utils": "latest",
    "@gluestack-ui/overlay": "latest",
    "@gluestack-ui/toast": "latest",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-clipboard/clipboard": "^1.14.1",
    "@react-native-community/netinfo": "11.3.1",
    "@tanstack/react-query": "^5.51.1",
    "@thirdweb-dev/react-native-adapter": "^1.2.0",
    "@trpc/client": "next",
    "@trpc/react-query": "next",
    "@trpc/server": "next",
    "@vercel/static-build": "^2.5.0",
    "amazon-cognito-identity-js": "^6.3.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "expo": "^51.0.8",
    "expo-application": "~5.9.1",
    "expo-build-properties": "~0.12.1",
    "expo-constants": "~16.0.1",
    "expo-image": "~1.12.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.14",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "lucide-react-native": "^0.350.0",
    "nativewind": "^4.0.36",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-native": "0.74.1",
    "react-native-aes-gcm-crypto": "^0.2.2",
    "react-native-get-random-values": "~1.11.0",
    "react-native-mmkv": "^2.12.2",
    "react-native-quick-crypto": "^0.7.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.6",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "thirdweb": "^5.34.3",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "compression": "^1.7.4",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "prisma": "^5.13.0",
    "react-native-react-query-devtools": "^1.1.0",
    "typescript": "~5.3.3"
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "node src/prisma/seed.js"
  },
  "private": true
}
